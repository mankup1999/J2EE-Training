package src.com.javafundamentals;

public class Demo13 {

	public static void main(String[] args) {
		
		if(args.length!=4) {
			System.out.println("Please enter 4 integer numbers");
			return;
		}
		
		int[][] res=new int[2][2];
		
		for(int i=0;i<2;i++)
			for(int j=0;i<2;j++)
				res[i][j]=Integer.parseInt(args[2*i+j]);
			
			
			

	}

}
